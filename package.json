{
  "contributors": [
    "Brendan Miller <info@thebrandingfarm.com>"
  ],
  "name": "vuetify2-jsonschema-form",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "yarn run build:lib",
    "build:demo": "vue-cli-service build",
    "build:lib": "vue-cli-service build --target lib --name VJsonschemaForm ./src/lib/index.vue",
    "lint": "vue-cli-service lint",
    "lint:fix": "vue-cli-service lint --fix"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "markdown-it": "^9.0.1",
    "match-all": "^1.2.5",
    "property-expr": "^1.5.1",
    "vue": "^2.6.10",
    "vue-color": "^2.7.0",
    "vue-swatches": "^1.0.3",
    "vuedraggable": "2.23.0",
    "vuetify": "^2.0.0-beta.9"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.2",
    "@vue/cli-plugin-eslint": "^3.9.2",
    "@vue/cli-service": "^3.9.3",
    "@vue/eslint-config-standard": "^4.0.0",
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.2",
    "deepmerge": "^4.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "eslint-plugin-vuetify": "^1.0.0-beta.1",
    "fibers": "^4.0.1",
    "hjson": "^3.1.2",
    "husky": "^3.0.1",
    "lint-staged": "^9.2.0",
    "sass": "^1.22.7",
    "sass-loader": "^7.1.0",
    "vue-axios": "^2.1.4",
    "vue-cli-plugin-vuetify": "^0.5.0",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.2.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "vue-cli-service lint --fix",
      "git add"
    ]
  }
}
